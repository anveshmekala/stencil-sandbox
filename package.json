    "test.e2e": "stencil test  --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "docs.data": "stencil build --docs-readme"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "@stencil/router": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/puppeteer": "^5.4.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jsdoc": "^3.6.7",
    "puppeteer": "^5.5.0"
  },
  "license": "MIT"
}
